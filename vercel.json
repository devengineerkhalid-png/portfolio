{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "vite --host",
  "installCommand": "npm install",
  "env": {
    "VITE_API_BASE_URL": "@api_base_url"
  },
  "routes": [
    {
      "src": "^/(?:assets)/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
